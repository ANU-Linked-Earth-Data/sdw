greg:April
  rdf:type owl:Class ;
  rdf:type time:MonthOfYear ;
  rdfs:label "April"@en ;
  rdfs:subClassOf time:DateTimeDescription ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue time:unitMonth ;
      owl:onProperty time:unitType ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue "--04" ;
      owl:onProperty time:month ;
    ] ;
