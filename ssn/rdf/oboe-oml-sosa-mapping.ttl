# baseURI: http://www.w3.org/ns/sosa/oboe-oml/
# imports: http://www.w3.org/ns/sosa/oml
# imports: http://www.w3.org/ns/sosa/sosa-oboe/

@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix oboe-core: <http://ecoinformatics.org/oboe/oboe.1.0/oboe-core.owl#> .
@prefix oml: <https://www.w3.org/ns/sosa/oml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

oboe-core:Context
  owl:equivalentClass oml:ObservationContext ;
.
oboe-core:Observation
  rdfs:comment "oboe-core:Observation is a subclass of oml:ObservationCollection with the contraint that all instances of the featureOfInterest of any member must be the same for anu one oboe-core:Observation. " ;
  rdfs:subClassOf oml:ObservationCollection ;
.
oboe-core:ObservationCollection
  rdfs:subClassOf oml:ObservationCollection ;
.
oboe-core:contextObservationFor
  owl:equivalentProperty oml:context ;
.
oboe-core:hasContext
  owl:equivalentProperty oml:context ;
.
oboe-core:hasContextObservation
  owl:equivalentProperty oml:relatedObservation ;
.
oboe-core:hasMeasurement
  rdfs:subPropertyOf rdfs:member ;
  rdfs:subPropertyOf oml:member ;
.
oboe-core:hasMember
  rdfs:subPropertyOf rdfs:member ;
  rdfs:subPropertyOf oml:member ;
.
oboe-core:measurementFor
  rdfs:subPropertyOf oml:memberOf ;
.
oboe-core:memberOf
  rdfs:subPropertyOf oml:memberOf ;
.
<http://www.w3.org/ns/sosa/oboe-oml/>
  rdf:type owl:Ontology ;
  rdfs:comment "Alignment of OBOE-core with the OML extension of SOSA.This mapping takes advantage of oml:ObservationContext and oml:ObservationCollection" ;
  rdfs:label "OBOE-Core - OML mapping" ;
  owl:imports sosa:oml ;
  owl:imports <http://www.w3.org/ns/sosa/sosa-oboe/> ;
.
